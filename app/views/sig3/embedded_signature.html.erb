<!-- Gotta include the HelloSign embedded.js library -->
<script type="text/javascript" src="https://s3.amazonaws.com/cdn.hellosign.com/public/js/hellosign-embedded.LATEST.min.js"></script>
<!-- Open the embedded signer iFrame -->
<script type="text/javascript">
// <![CDATA[
 HelloSign.init( '<%= @client_id %>');
  HelloSign.open({
    'url': '<%= @sign_url %>',
    'allowCancel': true,
    'skipDomainVerification': true,
    'uxVersion': 2,
    'redirectUrl': "http://www.hellosign.com/",
    'messageListener': function(eventData) {
    if (eventData.event == HelloSign.EVENT_SIGNED) {
        // Go to a signature confirmation page
        console.log (eventData["signature_id"] + " helllllloooooo");
    }
}
  });
// ]]>
</script>
